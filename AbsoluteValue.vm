function Main.AbsoluteValue 0
    push argument 0
    push constant 0
    lt
    if-goto NEGATIVE
    push argument 0
    pop argument 1
    push constant 0
    pop argument 2
    push constant 0
    pop argument 3
    return
label NEGATIVE
    push argument 0
    neg
    pop argument 1
    push constant 1
    pop argument 2
    push constant 0
    pop argument 3
    return
